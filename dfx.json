{
  "canisters": {
    "harvestx_backend": {
      "type": "rust",
      "package": "harvestx_backend",
      "candid": "src/HarvestX_backend/harvestx_backend.did"
    },
    "harvestx_frontend": {
      "type": "assets",
      "source": [
        "src/HarvestX_frontend/dist"
      ],
      "candid": "src/HarvestX_backend/harvestx_backend.did"
    }
  },
  "defaults": {
    "build": {
      "packtool": "cargo"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "version": 1
}
